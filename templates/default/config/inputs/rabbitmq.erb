  rabbitmq {
    ack => true
    host => '<%= @host %>'
    port => <%= @port %>
    user => "<%= @user %>"
    password => "<%= @password %>"
    ssl => <%=  @ssl_enabled %>
    auto_delete => false
    durable => true
    exchange => "logstash"
    exclusive => false
    queue => "logstash_queue"
    threads => 2
  }
